{
    "manifest_version": 3,
    "name": "Infinity New Tab",
    "version": "1.0.0",
    "description": "一个美观的新标签页，管理你的常用网站",
    "permissions": [
        "storage",
        "tabs",
        "downloads",
        "history"
    ],
    "host_permissions": [
        "https://api.unsplash.com/*",
        "https://images.unsplash.com/*",
        "https://api.ixiaowai.cn/*",
        "https://www.dmoe.cc/*"
    ],
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "icons": {
        "16": "icons/icon-16.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    }
}
